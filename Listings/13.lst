0056 1582             31                                     DEC DPL                         ;regresar al punto donde em
                             pieza la lista

                      32     

0058 75A200           33     SETMENOR:               MOV DPS,#0                      ;accedemos a la version del DPTR do
                             nde esta el más pequeño

005B E8               34                                     MOV A, R0

